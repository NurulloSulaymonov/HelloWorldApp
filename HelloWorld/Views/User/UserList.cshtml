@model List<SchoolSystem.Models.ViewModel.User>
<table class="table table-striped table-dark">
<thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">User</th>
        <th scope="col">Phone</th>
    </tr>
</thead>
@foreach (var user in Model)
{
    
    <tbody>
    <tr>
        <th scope="row">@user.Id</th>
        <td>@user.UserName</td>
        <td>@user.Phone</td>
    </tr>
    </tbody>
}
</table>